<div class="row">
	<div class="span6 offset3">
		<div class="well">
			<p>
			  <b>Description:</b>
			  <%= @protocol.description %>
			</p>
			<p>
			  <%= @protocol.user.name %>
			</p>
			<% if current_user == @protocol.user %>
				<%= link_to 'Edit', edit_protocol_path(@protocol) %> |
			<% end %>
			<%= link_to 'Back', protocols_path %>
		</div>
	</div>
</div>

<%= link_to 'New Protocol', new_protocol_path %>