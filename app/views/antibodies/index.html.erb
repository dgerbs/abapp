<h1>Listing Antibodies</h1>
<table class="table table-striped">
  <thead>
    <tr>
      <th>Target</th>
      <th>Vendor</th>
      <th>Product ID</th>
      <th>Description</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

<% @antibodies.each do |antibody| %>
  <tr>
    <td><%= antibody.target %></td>
    <td><%= antibody.vendor %></td>
    <td><%= antibody.product_ID %></td>
    <td><%= antibody.description %></td>
    <td><%= link_to 'Info', antibody, class: "btn btn-info" %></td>
  </tr>
<% end %>
</table>

<%= link_to 'New Antibody', new_antibody_path %>